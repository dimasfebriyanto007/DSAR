@using cms.Common
<!DOCTYPE html>
<html>
<head runat="server">
    <title>@ViewBag.Title :: DSAR Online</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <!-- TinyMCE -->
    <script type="text/javascript" src="@Url.Content("~/Scripts/tiny_mce/tiny_mce.js")"></script>    
    <!-- /TinyMCE -->   
</head>
<body>
    <div class="page">
        <div id="header">
            <div id="title">
                <h1>DSAR Online</h1>
            </div>
            <div id="logindisplay">
                @{Html.RenderPartial("LogOnUserControl");}                
            </div> 
            <div id="menucontainer">
                @{Html.RenderPartial("MenuUserControl");}                
            </div>
        </div>
        <div id="main">
            @RenderBody()
            <div id="footer">
            </div>
        </div>
    </div>
</body>
</html>